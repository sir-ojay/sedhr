{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/types/*": [
        "types/*"
      ],
      "@/styles/*": [
        "styles/*"
      ],
      "@/layouts/*": [
        "layouts/*"
      ],
      "@/config/*": [
        "config/*"
      ],
      "@/helpers/*": [
        "utilities/*"
      ],
      "@/data/*": [
        "data/*"
      ],
      "@/services/*": [
        "services/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "components/collaboration/rfp/ReviewnB"  ],
  "exclude": [
    "node_modules"
  ]
}
